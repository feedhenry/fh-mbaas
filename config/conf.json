{
  "fhmbaas": {
    "port": 8080,
    "key": "$(FHMBAAS_KEY)",
    "protocol": "https",
    "temp_forms_files_dest" : "/tmp/fh-mbaas",
    "pdfExportDir": "/tmp/fh-mbaas",
    "code_coverage_enabled": false,
    "logger": {
      "name": "mbaas",
      "streams": [{
        "type": "stream",
        "src": true,
        "level": "trace",
        "stream": "process.stdout"
      }, {
        "type": "raw",
        "src": true,
        "level": "trace",
        "stream": "ringBuffer"
      }]
    }
  },
  "mongo": {
    "mongoUrl" : "",
    "enabled": true,
    "name": "$(MONGODB_FHMBAAS_DATABASE)",
    "host": "$(exec.dig mongodb-service A +search +short)",
    "port": 27017,
    "replicaset_name": null,
    "auth": {
      "enabled": true,
      "user": "$(MONGODB_FHMBAAS_USER)",
      "pass": "$(MONGODB_FHMBAAS_PASSWORD)"
    },
    "admin_auth": {
      "user": "$(MONGODB_ADMIN_USER)",
      "pass": "$(MONGODB_ADMIN_PASSWORD)"
    }
  },
  "fhditch": {
    "host": "localhost",
    "port": 8802,
    "protocol": "http"
  },
  "fhdfc": {
    "enbaled": false,
    "dynofarm": "http://localhost:9000",
    "username":"feedhenry",
    "_password": "feedhenry101",
    "loglevel": "warn",
    "cache_timeout": 300000
  },
  "fhamqp":{
    "enabled": true,
    "max_connection_retry": 10,
    "nodes":"localhost:5672",
    "ssl": false,
    "vhosts":{
      "events":{
        "name":"fhevents",
        "user":"fheventuser",
        "password":"fheventpassword"
      }
    },
    "app":{
      "enabled": true
    }
  },
  "fhmessaging":{
    "enabled": true,
    "host":"localhost",
    "protocol":"http",
    "port":8803,
    "path":"msg/TOPIC",
    "cluster":"development",
    "realtime": false,
    "files":{
      "recovery_file":"../messages/recovery.log",
      "backup_file":"../messages/backup.log"
    }
  },
  "fhstats":{
    "enabled": true,
    "host":"localhost",
    "port": 8804,
    "protocol": "http"
  },
  "fhredis":{
    "host": "$(REDIS_SERVICE_SERVICE_HOST)",
    "port": 6379,
    "password":""
  }
}
